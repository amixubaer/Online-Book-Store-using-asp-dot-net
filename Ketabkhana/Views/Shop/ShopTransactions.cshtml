@model IEnumerable<Ketabkhana.Models.EF.Transaction>

@{
    ViewBag.Title = "ShopTransactions";
}

<h2>Shop Transactions History</h2>

<table id="table">
    <tr>
        <th>ID</th>
        <th>Shop Username</th>
        <th>Customer Username</th>
        <th>Amount</th>

    </tr>

    @foreach (var b in Model)
    {
        <tr>
            <td>@b.id</td>
            <td>@b.shopUsername</td>
            <td>@b.customerUsername</td>
            <td>@b.amount</td>

        </tr>
    }

</table>