@model IEnumerable<Ketabkhana.Models.EF.Transaction>
@{
    ViewBag.Title = "AllTransactions";
}

<h2> Transactions History</h2>

<a href="/Employee/Addtransaction" class="btn btn-success">Add New</a>
<br />
<br />

<table  id="table">
    <tr>
        <th>Shop</th>
        <th>Customer</th>
        <th>Amount</th>

    </tr>

    @foreach (var t in Model)
    {
        <tr>
            <td>@t.shopUsername</td>
            <td>@t.customerUsername</td>
            <td>@t.amount</td>

        </tr>
    }
</table>