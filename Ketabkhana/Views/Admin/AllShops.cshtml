@model IEnumerable<Ketabkhana.Models.VM.ShopModel>
@{
    ViewBag.Title = "AllShops";
}

<h2>All Shops</h2>

<table  id="table">
    <tr>
        <th>ID</th>
        <th>Shop Name</th>
        <th>Shop Address</th>
        <th>Email</th>
        <th>Username</th>
        <th>Action</th>
    </tr>
    @foreach (var s in Model)
    {
        <tr>
            <td>@s.id</td>
            <td>@s.shopName</td>
            <td>@s.shopAddress</td>
            <td>@s.email</td>
            <td>@s.username</td>
            <td>
                <a href="/Admin/UpdateShop/@s.id" class="btn btn-success">Edit</a>
                <a href="/Admin/DeleteShop/@s.id" class="btn btn-danger">Delete</a>
            </td>
        </tr>
    }
</table>