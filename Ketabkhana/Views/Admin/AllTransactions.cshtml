@model IEnumerable<Ketabkhana.Models.EF.Transaction>
@{
    ViewBag.Title = "AllTransactions";
}

<h2>Transaction History</h2>

<table  id="table">
    <tr>
        <th>Shop Username</th>
        <th>Customer Username</th>
        <th>Amount</th>
    </tr>
    @foreach (var t in Model)
    {
        <tr>
            <td>@t.shopUsername</td>
            <td>@t.customerUsername</td>
            <td>@t.amount</td>
        </tr>
    }
</table>