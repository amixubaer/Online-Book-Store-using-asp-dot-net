@model IEnumerable<Ketabkhana.Models.EF.Admin>
@{
    ViewBag.Title = "AllAdmins";
}

<h2>All Admins</h2>


<a href="/Admin/AddAdmin" class="btn btn-success">Add New</a>
<br />
<br />

<table  id="table">
    <tr>
        <th>ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Gender</th>
        <th>Dob</th>
        <th>Email</th>
        <th>Username</th>
        <th>Action</th>
    </tr>
    @foreach (var a in Model)
    {
        <tr>
            <td>@a.id</td>
            <td>@a.firstName</td>
            <td>@a.lastName</td>
            <td>@a.gender</td>
            <td>@a.dob</td>
            <td>@a.email</td>
            <td>@a.username</td>
            <td>
                <a href="/Admin/UpdateAdmin/@a.id" class="btn btn-success">Edit</a>
                <a href="/Admin/DeleteAdmin/@a.id" class="btn btn-danger">Delete</a>
            </td>
        </tr>
    }
</table>